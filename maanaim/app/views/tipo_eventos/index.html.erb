<table class="table">
  <thead>
    <tr>
      <th>Nome</th>
      <th>Ativo</th>
      <th>Dtcadastro</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_eventos.each do |tipo_evento| %>
      <tr>
        <td><%= tipo_evento.nome %></td>
        <td><%= tipo_evento.ativo %></td>
        <td><%= tipo_evento.dtCadastro %></td>
        <td><%= link_to 'Edit', edit_tipo_evento_path(tipo_evento), remote: true %></td>
        <td><%= link_to 'Destroy', tipo_evento, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
